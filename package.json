    "gulp-debug": "^4.0.0",
    "semantic-release": "^17.0.6",
    "snazzy": "^8.0.0",
    "standard": "^14.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nodewell",
    "asset",
