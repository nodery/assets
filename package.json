  "license": "(ISC OR CC-BY-4.0)",
  "homepage": "https://github.com/nodewell/assets#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodewell/assets.git"
  },
  "bugs": {
    "url": "https://github.com/nodewell/assets/issues"
